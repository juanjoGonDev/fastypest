{
  "testEnvironment": "node",
  "preset": "@swc-node/jest",
  "rootDir": ".",
  "setupFilesAfterEnv": [
    "./config/jest.setup.ts"
  ],
  "globalSetup": "./config/global.setup.ts",
  "testRegex": ".spec.ts",
  "transform": {
    "^.+\\.tsx?$": [
      "@swc-node/jest",
      {
        "tsconfig": "tsconfig.json"
      }
    ]
  }
}
